{
    "name": "Learn Enhancer",
    "version": "1.10.2",
    "manifest_version": 2,
    "content_scripts": [
    {
      "matches": ["http://learn.canterbury.ac.nz/*", "http://login.canterbury.ac.nz/*", "https://blackboard.vuw.ac.nz/*"],
      "js": ["domready.js", "pranks.js", "content.js"],
      "css" : ["content.css"],
      "run_at": "document_start"
    }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "webRequestBlocking",
      "webRequest",
      "storage",
      "http://learn.canterbury.ac.nz/*",
      "https://login.canterbury.ac.nz/*"
    ],
    "icons": {"48": "48.png",
              "128": "128.png" },
    "options_page": "options.html"

}
